  "battery": {
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "<span size='13000' foreground='#11111b'>{icon} </span> {capacity}%",
    "format-warning": "<span size='13000' foreground='#11111b'>{icon} </span> {capacity}%",
    "format-critical": "<span size='13000' foreground='#11111b'>{icon} </span> {capacity}%",
    "format-charging": "<span size='13000' foreground='#11111b'>ï‡¦ </span>{capacity}%",
    "format-plugged": "<span size='13000' foreground='#11111b'>ï‡¦ </span>{capacity}%",
    "format-alt": "<span size='13000' foreground='#11111b'>{icon} </span> {time}",
    "format-full": "<span size='13000' foreground='#11111b'>ï‰€ </span>{capacity}%",
    "format-icons": [
      "ï‰„",
      "ï‰ƒ",
      "ï‰‚",
      "ï‰",
      "ï‰€"
    ],
    "tooltip-format": "{time}"
  },
  "network": {
    "format-wifi": "<span size='13000' foreground='#f5e0dc'>ï‡«  </span>{essid}",
    "format-ethernet": "<span size='13000' foreground='#f5e0dc'>ó°¤­  </span> Disconnected",
    "format-linked": "{ifname} (No IP) ï–",
    "format-disconnected": "<span size='13000' foreground='#f5e0dc'>ïª  </span>Disconnected",
    "tooltip-format-wifi": "Signal Strenght: {signalStrength}%"
  },
